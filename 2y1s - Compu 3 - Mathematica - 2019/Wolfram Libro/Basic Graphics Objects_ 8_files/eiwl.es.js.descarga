function getCloudNotebookURL(section) {
console.log(section);
    var page = section.replace(/\.\w\w$/, "");
    var url = "https://develop.wolframcloud.com/app/objects/wolfram-media/Elementary-Introduction-to-the-Wolfram-Language/InteractiveNotebooks/EIWL2-" + page + ".nb?_view=frameless";
//    console.log(url);
    return url;
}

function getMOOCURL(section) {
    var baseUrl = get_baseUrl();
    var page = section.replace(/\.\w\w$/, "");
    var url = baseUrl + "/wolfram-u/s/an-elementary-introduction-to-the-wolfram-language/" + page + ".html";
    console.log(baseUrl);
    console.log(url);
    return url;
}



function get_mech() {
   /********************
    
      WHICH MACHINE?
      
   ********************/
    
   var devReg = new RegExp('devel2');
   var testReg = new RegExp('test');
   var urlStr = "" + window.location.host;
   var insertStr = "";

   if (urlStr.match(devReg)) {
      insertStr = "devel2.";
//      insertStr = "test.";
   } else if (urlStr.match(testReg)) {
      insertStr = "test.";
   }
console.log(urlStr);
   return insertStr
}       
        
            
function get_baseUrl() {
             
   var mech = get_mech();
   /* construct specific url */
   return "http://www."+ mech +"wolfram.com"
}       
      

      
function GetFileName(){
//this gets the full url
var url = document.location.href;
//this removes the anchor at the end, if there is one
url = url.substring(0, (url.indexOf("#") == -1) ? url.length : url.indexOf("#"));
//this removes the query after the file name, if there is one
url = url.substring(0, (url.indexOf("?") == -1) ? url.length : url.indexOf("?"));
//this removes everything before the last slash in the path
url = url.substring(url.lastIndexOf("/") + 1, url.length);
//return
if(url.length == 0){
url = "index";
}  
return url;
}




