function next(elem) {
    do {
        elem = elem.nextSibling;
    } while (elem && elem.nodeType !== 1);
    return elem;
};
function OpenCloseExercises(id){
        var exer = document.getElementById(id);
        var input = next(exer);
        if(input.className.match(/(?:^|\s)ExerciseInputBox(?!\S)/) ) {
                if ( input.className.match(/(?:^|\s)active(?!\S)/) ) {
                        input.className = 'ExerciseInputBox';
                } else {
                        input.className += " " + 'active';
                }
        }
};



