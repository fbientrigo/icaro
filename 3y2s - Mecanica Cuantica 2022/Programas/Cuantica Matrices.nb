Cell["\<\
Sisteigen[matrix, imprimir: True]
Retorna {\[Lambda],\[Nu]} de la misma forma que presentamos el elemento; \
posee una opcion forceflag para forzar el uso de un operador no hermitico, \
sin embargo aun no esta implementado el trabajo con estos;
que sucede con la degenerancia?\
\>", "Text",
 CellChangeTimes->{{3.812630516461561*^9, 
  3.8126305960117817`*^9}},ExpressionUUID->"782df03f-b5ce-42af-9ee0-\
4c6c902defc8"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Sisteigen", "[", 
   RowBox[{"M_", ",", 
    RowBox[{"p_", ":", "True"}]}], "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"HermitianMatrixQ", "[", "M", "]"}], " ", "||", " ", 
      "forceflag"}], " ", ",", "\[IndentingNewLine]", 
     RowBox[{"(", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\[Lambda]", ",", "\[Nu]"}], "}"}], " ", "=", "  ", 
        RowBox[{"Eigensystem", "[", "M", "]"}]}], ";", "\[IndentingNewLine]", 
       
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\[Lambda]", ",", "\[Nu]"}], "}"}], "=", 
        RowBox[{"Transpose", "@", 
         RowBox[{"SortBy", "[", 
          RowBox[{
           RowBox[{"Transpose", "[", 
            RowBox[{"{", 
             RowBox[{"\[Lambda]", ",", "\[Nu]"}], "}"}], " ", "]"}], ",", 
           "First"}], "]"}]}]}], ";", "  ", 
       RowBox[{"(*", 
        RowBox[{
        "Esto", " ", "deja", " ", "cada", " ", "eigenval", " ", "con", " ", 
         "su", " ", "eigenvec"}], "*)"}], "\[IndentingNewLine]", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
        "obtien", " ", "los", " ", "valores", " ", "y", " ", "vectores", " ", 
         "propios", " ", "de", " ", "la", " ", "matrix"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"\[Nu]normc", " ", "=", " ", 
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Sqrt", "[", 
            RowBox[{"Abs", "[", 
             RowBox[{
              RowBox[{"#", "\[Conjugate]"}], ".", "#"}], "]"}], "]"}], "&"}], 
          ",", "\[Nu]"}], "]"}]}], " ", ";", "    ", 
       RowBox[{"(*", 
        RowBox[{
        "factores", " ", "de", " ", "norma", " ", "para", " ", "eigen", " ", 
         "vectores"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"\[Nu]", " ", "=", " ", 
        RowBox[{"FullSimplify", "[", 
         RowBox[{"\[Nu]", "/", "\[Nu]normc"}], "]"}]}], ";", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\[Lambda]", ",", "\[Nu]"}], "}"}], "=", " ", 
        RowBox[{"{", 
         RowBox[{"\[Lambda]", ",", 
          RowBox[{"Reverse", "[", "\[Nu]", "]"}]}], "}"}]}], ";", " ", 
       RowBox[{"(*", 
        RowBox[{
        "Se", " ", "me", " ", "invertia", " ", "el", " ", "vector", " ", 
         "asi", " ", "que", " ", "esto", " ", "hara", " ", "una", " ", 
         "correspondencia", " ", "1", " ", "a", " ", "1"}], "*)"}], 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"dim", " ", "=", " ", 
        RowBox[{"Length", "[", 
         RowBox[{"\[Nu]", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]",
        "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{
        "\"\<Dimension: \>\"", ",", "dim", ",", " ", 
         "\"\< Con eigen val \[Lambda]: \>\"", ",", " ", "\[Lambda]"}], "]"}],
        ";", "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Los vectores \[Nu] son:\>\"", ",", " ", "\[Nu]"}], "]"}],
        ";", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", 
        RowBox[{"{", 
         RowBox[{"\[Lambda]", ",", "\[Nu]"}], "}"}], "]"}]}], ")"}], 
     "\[IndentingNewLine]", ",", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
       "Print", "[", 
        "\"\<Es una matriz no hermitiana, desea continuar?: \>\"", "]"}], ";",
        "\[IndentingNewLine]", 
       RowBox[{"Button", "[", 
        RowBox[{"\"\<Forzar operacion no hermiticidad: \>\"", ",", " ", 
         RowBox[{"(", 
          RowBox[{"forceflag", "=", "True"}], ")"}]}], "]"}]}], 
      "\[IndentingNewLine]", ")"}]}], "]"}], ")"}]}]], "Input",
 InitializationCell->True,
