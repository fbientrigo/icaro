Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Comprb", "[", 
    RowBox[{
    "\[Psi]_", ",", "\[Psi]p_", ",", "M_", ",", "\[Lambda]p_", ",", 
     "\[Nu]p_"}], "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{"mediciones", ",", 
     RowBox[{"(", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"mediciones", " ", "=", " ", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"\[Lambda]p", "[", 
            RowBox[{"[", "j", "]"}], "]"}], " ", 
           RowBox[{"\[Psi]p", "[", 
            RowBox[{"[", "j", "]"}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "1", ",", "dim"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{"MatrixForm", "[", "mediciones", "]"}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{
         RowBox[{"MatrixForm", "[", "\[Nu]p", "]"}], ",", 
         "\"\< Cada fila corresponde a un eigenvector \[Nu]\>\""}], "]"}], 
       ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"mediciones", " ", ".", "\[Nu]p"}], " ", "\[Equal]", " ", 
          RowBox[{"M", ".", "\[Psi]"}]}], ",", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{
            "Print", "[", 
             "\"\<Multiplicar la matrix por el vector ha dado el mismo \
resultado que el camino de los coeficientes)\>\"", "]"}], 
            "\[IndentingNewLine]", 
            RowBox[{"Return", "[", "True", "]"}]}], ";"}], 
          "\[IndentingNewLine]", ")"}], ",", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{
            "\"\<Los calculos de la matrix por el vector son distintos, \
revisemos los coeficientes y el vector\>\"", ",", "\[Psi]", ",", "\[Nu]"}], 
            "]"}], ";", "\[IndentingNewLine]", 
           RowBox[{"Return", "[", "False", "]"}], ";"}], 
          "\[IndentingNewLine]", ")"}]}], "]"}]}], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", ")"}]}], "]"}]}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", "\[IndentingNewLine]"}]], "Input",
