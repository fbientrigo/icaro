Cell[BoxData[
 RowBox[{
  RowBox[{"Stvec", "[", 
   RowBox[{"x_", ",", "y_", ",", "z_", ",", 
    RowBox[{"c_", ":", "1"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"coef", ",", "const"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"const", "=", "c"}], ";", "\[IndentingNewLine]", 
      RowBox[{"coef", " ", "=", " ", 
       RowBox[{"c", "*", 
        RowBox[{"{", 
         RowBox[{"x", ",", "y", ",", "z"}], "}"}]}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"\[Psi]", "=", 
       RowBox[{
        RowBox[{
         FractionBox["1", 
          RowBox[{"\[Sqrt]", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"coef", "\[Conjugate]"}], ".", "coef"}], ")"}]}]], 
         "coef"}], " ", "//", "FullSimplify"}]}], ";", "\[IndentingNewLine]", 
      
      RowBox[{"Return", "[", "\[Psi]", "]"}], ";"}], ")"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
